{
  "name": "@waitlist/server",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "ts-node scripts/clean.ts",
    "db:init": "run-s \"db:migrate-save -- --name 'initial version'\" db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "package:container": "docker build .",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down --volumes"
  },
  "dependencies": {
    "@nestjs/common": "10.0.4",
    "@nestjs/config": "1.2.1",
    "@nestjs/core": "10.0.4",
    "@nestjs/graphql": "9.2.7",
    "@nestjs/platform-express": "10.0.4",
    "@nestjs/serve-static": "4.0.0",
    "@nestjs/swagger": "5.2.1",
    "@types/bcrypt": "5.0.0",
    "@prisma/client": "4.16.2",
    "apollo-server-express": "3.12.0",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "graphql": "15.8.0",
    "graphql-type-json": "0.3.2",
    "nest-access-control": "2.2.0",
    "nest-morgan": "2.0.0",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.6.3",
    "ts-node": "10.9.1",
    "validator": "^13.9.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^9.0.3",
    "passport": "0.6.0",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.8",
    "@nestjs/testing": "10.0.4",
    "@types/express": "4.17.17",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "26.0.24",
    "@types/normalize-path": "3.0.0",
    "@types/supertest": "2.0.12",
    "@types/validator": "^13.7.17",
    "jest": "27.5.1",
    "jest-mock-extended": "^2.0.9",
    "prisma": "4.16.2",
    "supertest": "6.3.3",
    "ts-jest": "27.1.5",
    "type-fest": "3.12.0",
    "typescript": "4.9.5",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": ["<rootDir>/dist/"]
  }
}
